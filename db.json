                        "makeNewBed":{
                            "process":[
                                "remove all sheets from beds and room",                                
                                "place all old sheets into cart's soiled linen basket",
                                "select sheet sets that match the size and dimensions of the bed",
                                "place new fitted sheet onto mattress",
                                "place new flat sheet on top of fitted sheet",
                                "fold head of flat sheet down 12 inches towards foot of bed",
                                "tuck in the remaining 3 sides of flat sheet under mattress",
                                "place new pillow cases on all pillows",
                                "position 2 pillows, side by side at the head of bed",
                                "if more than two pillows are on bed, then place extras in closet dust bag",
                                "place new comforter on top of bed, covering evenly",
                                "place new comforter pillow cases onto decorative pillows",
                                "place decorative pillows at the head of bed, on top of comforter"
